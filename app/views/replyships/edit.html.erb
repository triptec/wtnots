<h1>Editing replyship</h1>

<% form_for(@replyship) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :comment_id %><br />
    <%= f.text_field :comment_id %>
  </p>
  <p>
    <%= f.label :reply_id %><br />
    <%= f.text_field :reply_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @replyship %> |
<%= link_to 'Back', replyships_path %>